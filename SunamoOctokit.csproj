<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <TargetFramework>net8.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Version>24.7.16.1</Version>
        <Description>Wrapper around Octokit library</Description>
        <Authors>www.sunamo.cz</Authors>
        <Copyright>www.sunamo.cz</Copyright>
        <PackageProjectUrl>https://sunamo.cz/</PackageProjectUrl>
        <RepositoryUrl>https://github.com/sunamo/SunamoOctokit.git</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <PackageReadmeFile>readme.md</PackageReadmeFile>
    </PropertyGroup>
    <ItemGroup>
        <PackageReference Include="Octokit" Version="9.1.2"/>
        <None Include="readme.md" Pack="True" PackagePath="."/>
    </ItemGroup>
    <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
        <DefineConstants>$(DefineConstants);ASYNC;SunamoOctokit;SunamoOctokit.csproj</DefineConstants>
    </PropertyGroup>
    <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
        <DefineConstants>$(DefineConstants);ASYNC;SunamoOctokit;SunamoOctokit.csproj</DefineConstants>
    </PropertyGroup>
</Project>